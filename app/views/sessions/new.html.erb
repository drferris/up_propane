<h1>Sign In</h1>

<div class='row'>
	<div class='span6 offset3'>
		<%= form_tag('#', method: 'post') do %>

			<%= label_tag 'email' %>
			<%= text_field_tag 'email' %>

			<%= label_tag 'password' %>
			<%= password_field_tag 'password' %>

			<%= submit_tag 'Sign In', class: 'btn btn-medium btn-primary' %>
		<% end %>
	</div>
</div>